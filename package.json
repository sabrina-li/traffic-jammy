{
  "name": "traffic-jammy",
  "version": "1.0.0",
  "description": "a traffic data visualization app",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\" \"mongod --config /usr/local/etc/mongod.conf\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sabrina-li/traffic-jammy.git"
  },
  "keywords": [
    "traffid",
    "data",
    "visualization"
  ],
  "author": "Sabrina L",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sabrina-li/traffic-jammy/issues"
  },
  "homepage": "https://github.com/sabrina-li/traffic-jammy#readme",
  "dependencies": {
    "@mapbox/polyline": "^1.1.0",
    "@material-ui/core": "^4.2.0",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "classnames": "^2.2.6",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "isomorphic-fetch": "^2.2.1",
    "js-regression": "^1.0.10",
    "k-means": "^1.0.0",
    "mongoose": "^5.6.2",
    "node-sass": "^4.12.0",
    "prop-types": "^15.7.2",
    "react-controllables": "^0.6.0",
    "react-google-maps": "^9.4.5",
    "react-places-autocomplete": "^7.2.1",
    "react-router-dom": "^5.0.1",
    "recompose": "^0.30.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.1",
    "dotenv": "^8.0.0",
    "nodemon": "^1.19.1"
  }
}
